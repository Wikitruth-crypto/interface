{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@dapp/*": ["./src/*"],
      "@styles/*": ["./src/styles/*"],
      "@assets/*": ["./src/assets/*"],
      "@Create/*": ["./src/pages/Create/*"],
      "@Marketplace/*": ["./src/pages/Marketplace/*"],
      "@BoxDetail/*": ["./src/pages/BoxDetail/*"],
      "@Profile/*": ["./src/pages/Profile/*"],
      "@Token/*": ["./src/pages/Token/*"],
      "@Staking/*": ["./src/pages/Staking/*"],
      "@Dao/*": ["./src/pages/Dao/*"],
      "@supabaseDocs/*": ["./supabaseDocs/*"],

    }
  },
  "include": ["src", "vite.config.ts", "project.ts"],
  "exclude": ["node_modules", "dist"]
}




